{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
      
    ],
    "functions": {
        "api/classify.js": {
            "maxDuration": 60
        }
    }
  }
  